
<section id="stats">
<h3> <%= @astronauts.get_average_age %> </h3>
</section>

<% @astronauts.each do |astronaut| %>
<h1> <%= astronaut.name %> </h1>
<h1> <%= astronaut.age %> </h1>
<h1> <%= astronaut.job %> </h1>
<% end %>

<section id="missions">
  <h1>Missions</h1>

  <% @missions.each do |mission| %>
  <h5> <%= mission.title %> </h5>
  <% end %>
</section>
